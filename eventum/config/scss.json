{
  "eventum": {
    "output_folder": "css/gen/",
    "depends": [
    ],
    "rules": {
      "scss_eventum": {
        "output": "eventum/eventum.css",
        "inputs": [
          "eventum_scss/admin.scss",
          "eventum_scss/home/*.scss"
        ]
      },

      "scss_auth": {
        "output": "eventum/auth.css",
        "inputs": [ "eventum_scss/auth/*.scss" ]
      },

      "scss_users": {
        "output": "eventum/users.css",
        "inputs": [ "eventum_scss/users/*.scss" ]
      },

      "scss_events": {
        "output": "eventum/events.css",
        "inputs": [ "eventum_scss/events/*.scss" ]
      },

      "scss_posts": {
        "output": "eventum/posts.css",
        "inputs": [ "eventum_scss/posts/*.scss" ]
      },

      "scss_media": {
        "output": "eventum/amedia.css",
        "inputs": [ "eventum_scss/media/*.scss" ]
      }
    }
  },

  "lib": {
    "output_folder": "css/lib/",
    "depends": [],
    "rules": {

      "scss_datetime": {
        "output": "datetime.css",
        "inputs": [
          "eventum_scss/lib/bootstrap-datepicker.scss",
          "eventum_scss/lib/jquery.timepicker.scss"
        ]
      }
    }
  }
}
